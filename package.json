{
  "name": "alula.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-image-export-optimizer",
    "start": "next start",
    "pages:build": "yarn exec next-on-pages",
    "preview": "yarn run pages:build && wrangler pages dev",
    "deploy": "yarn run pages:build && wrangler pages deploy",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore --ignore-path .prettierignore ."
  },
  "type": "module",
  "packageManager": "yarn@4.9.4",
  "dependencies": {
    "next": "^15.4.5",
    "next-image-export-optimizer": "^1.19.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.13",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vercel": "^44.6.5",
    "wrangler": "^4.27.0"
  }
}
